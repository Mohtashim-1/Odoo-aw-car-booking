<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Car Booking Sequences -->
        
        <!-- Default Car Booking Sequence -->
        <record id="seq_car_booking" model="ir.sequence">
            <field name="name">Car Booking</field>
            <field name="code">car.booking</field>
            <field name="prefix">DSL/</field>
            <field name="padding">5</field>
            <field name="number_next">36</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Car Booking with Driver Sequence -->
        <record id="seq_car_booking_with_driver" model="ir.sequence">
            <field name="name">Car Booking with Driver</field>
            <field name="code">car.booking.with_driver</field>
            <field name="prefix">DSL/</field>
            <field name="padding">5</field>
            <field name="number_next">36</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Car Booking Rental Sequence -->
        <record id="seq_car_booking_rental" model="ir.sequence">
            <field name="name">Car Booking Rental</field>
            <field name="code">car.booking.rental</field>
            <field name="prefix">DSL/</field>
            <field name="padding">5</field>
            <field name="number_next">36</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo> 